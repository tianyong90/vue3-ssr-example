{
  "name": "vue3-ssr-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "cross-env NODE_ENV=production vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env NODE_ENV=production vite build --ssr src/entry-server.js --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:stylelint": "stylelint **/*.{vue,css,scss} --fix --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:prettier": "prettier --write --loglevel warn \"./**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.5",
    "@heroicons/vue": "^1.0.6",
    "@vueuse/core": "^9.0.0",
    "@vueuse/head": "^0.7.6",
    "animate.css": "^4.1.1",
    "async-validator": "^4.1.1",
    "axios": "^0.27.2",
    "bin-wrapper": "npm:bin-wrapper-china",
    "countup.js": "^2.2.0",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.1",
    "drift-zoom": "^1.5.0",
    "gsap": "^3.10.4",
    "headroom.js": "^0.12.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.14",
    "sweetalert2": "^11.4.17",
    "swiper": "^8.1.3",
    "tailwindcss": "^3.0.24",
    "vue": "^3.2.33",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "@types/video.js": "^7.3.39",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "express": "^4.18.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "^1.4.1",
    "sass": "^1.52.3",
    "serve-static": "^1.15.0",
    "sitemap": "^7.1.1",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vite": "^3.0.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.39.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  }
}
